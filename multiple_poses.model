# --base_lr 0.010000 --momentum 0.900000 --weight_decay 0.001000 -- solver SGD

layer {
  name: "data"
  type: "MolGridData"
  top: "data"
  top: "label"
  include {
    phase: TEST
  }
  molgrid_data_param {
        source: "smalltest0.types"
        batch_size: 50
        dimension: 23.5
        resolution: 0.500000
        shuffle: false
        balanced: false
        root_folder: "DATA_ROOT"
        num_poses: 9
        duplicate_poses: true
    }
  }
  
layer {
  name: "data"
  type: "MolGridData"
  top: "data"
  top: "label"
  include {
    phase: TRAIN
  }
  molgrid_data_param {
        source: "smalltrain0.types"
        batch_size:  50
        dimension: 23.5
        resolution: 0.500000
        shuffle: true
        balanced: true
        jitter: 0.000000        
        stratify_receptor: true
        stratify_affinity_min: 0
        stratify_affinity_max: 0
        stratify_affinity_step: 1.000000
        random_rotation: true
        random_translate: 6
        root_folder: "DATA_ROOT"       
        num_poses: 9
        duplicate_poses: true
    }
}

layer {
  name: "unit1_pool"
  type: "Pooling"
  bottom: "data"
  top: "unit1_pool"
  pooling_param {
    pool: AVE
    kernel_size: 2
    stride: 2
  }
}
layer {
  name: "unit1_conv"
  type: "Convolution"
  bottom: "unit1_pool"
  top: "unit1_conv"
  convolution_param {
    num_output: 32
    pad: 1
    kernel_size: 3
    stride: 1
    weight_filler {
      type: "xavier"
      symmetric_fraction: 1.0      
    }
  }
}
layer {
  name: "unit1_func"
  type: "ReLU"
  bottom: "unit1_conv"
  top: "unit1_conv"
  
}

layer {
  name: "unit2_conv"
  type: "Convolution"
  bottom: "unit1_conv"
  top: "unit2_conv"
  convolution_param {
    num_output: 32
    pad: 0
    kernel_size: 1
    stride: 1
    weight_filler {
      type: "xavier"
      symmetric_fraction: 1.0      
    }
  }
}
layer {
  name: "unit2_func"
  type: "ReLU"
  bottom: "unit2_conv"
  top: "unit2_conv"
  
}

layer {
  name: "unit3_pool"
  type: "Pooling"
  bottom: "unit2_conv"
  top: "unit3_pool"
  pooling_param {
    pool: AVE
    kernel_size: 2
    stride: 2
  }
}
layer {
  name: "unit3_conv"
  type: "Convolution"
  bottom: "unit3_pool"
  top: "unit3_conv"
  convolution_param {
    num_output: 64
    pad: 1
    kernel_size: 3
    stride: 1
    weight_filler {
      type: "xavier"
      symmetric_fraction: 1.0      
    }
  }
}
layer {
  name: "unit3_func"
  type: "ReLU"
  bottom: "unit3_conv"
  top: "unit3_conv"
  
}

layer {
  name: "unit4_conv"
  type: "Convolution"
  bottom: "unit3_conv"
  top: "unit4_conv"
  convolution_param {
    num_output: 64
    pad: 0
    kernel_size: 1
    stride: 1
    weight_filler {
      type: "xavier"
      symmetric_fraction: 1.0      
    }
  }
}
layer {
  name: "unit4_func"
  type: "ReLU"
  bottom: "unit4_conv"
  top: "unit4_conv"
  
}

layer {
  name: "unit5_pool"
  type: "Pooling"
  bottom: "unit4_conv"
  top: "unit5_pool"
  pooling_param {
    pool: AVE
    kernel_size: 2
    stride: 2
  }
}
layer {
  name: "unit5_conv"
  type: "Convolution"
  bottom: "unit5_pool"
  top: "unit5_conv"
  convolution_param {
    num_output: 128
    pad: 1
    kernel_size: 3
    stride: 1
    weight_filler {
      type: "xavier"
      symmetric_fraction: 1.0      
    }
  }
}
layer {
  name: "unit5_func"
  type: "ReLU"
  bottom: "unit5_conv"
  top: "unit5_conv"
  
}
layer {
    name: "split"
    type: "Split"
    bottom: "unit5_conv"
    top: "split"
}
layer {
  name: "output"
  type: "Softmax"
  bottom: "split"
  top: "output"
}
layer {
  name: "final_pool"
  type: "Pooling"
  bottom: "output"
  top: "final_pool"
  pooling_param {
    pool: MAX
    kernel_size: 9
    stride: 9
  }
}
layer {
  name: "loss"
  type: "SoftmaxWithLoss"
  bottom: "final_pool"
  bottom: "label"
  top: "loss"
}
layer {
  name: "reshape"
  type: "Reshape"
  bottom: "final_pool"
  top: "reshape"
  reshape_param {
    shape {
      dim: 50 dim: 9 dim: 128
    }
  }
}
layer {
  name: "pose_output"
  type: "InnerProduct"
  bottom: "reshape"
  top: "pose_output"
  inner_product_param {
    num_output: 2
    weight_filler {
      type: "xavier"
    }
  }
}
layer {
  name: "labelout"
  type: "Split"
  bottom: "label"
  top: "labelout"
  include {
    phase: TEST
  }
}
